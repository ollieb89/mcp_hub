{
  "mcpServers": {
    "serena": {
      "command": "uv",
      "args": ["run", "--directory", "/home/ob/Development/Tools/mcps/serena", "serena-mcp-server"],
      "cwd": ".",
      "env": {},
      "disabled": false
    },
    "vercel": {
      "type": "sse",
      "url": "https://mcp.vercel.com",
      "headers": {},
      "disabled": true
    },
    "shadcn-ui": {
      "command": "npx",
      "args": ["-y", "@jpisnice/shadcn-ui-mcp-server"],
      "cwd": ".",
      "env": {
        "GITHUB_TOKEN": "${GITHUB_TOKEN}"
      },
      "disabled": false
    },
    "gemini": {
      "command": "npx",
      "args": ["-y", "gemini-mcp-tool"],
      "cwd": ".",
      "env": {},
      "disabled": false
    }
  }
}

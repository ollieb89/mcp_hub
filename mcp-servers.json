{
  "mcpServers": {
    "serena": {
      "command": "uv",
      "args": ["run", "--directory", "/home/ob/Development/Tools/mcps/serena", "serena-mcp-server"],
      "cwd": ".",
      "env": {},
      "disabled": false
    },
    "vercel": {
      "type": "sse",
      "url": "https://mcp.vercel.com",
      "headers": {},
      "disabled": true
    },
    "shadcn-ui": {
      "command": "npx",
      "args": ["-y", "@jpisnice/shadcn-ui-mcp-server"],
      "cwd": ".",
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      },
      "disabled": false
    },
    "gemini": {
      "command": "npx",
      "args": ["-y", "gemini-mcp-tool"],
      "cwd": ".",
      "env": {
        "GOOGLE_API_KEY": "${GEMINI_API_KEY}"
      },
      "disabled": false
    },
    "docker-hub": {
      "command": "node",
      "args": ["/PATH/TO/docker-hub-mcp-server/dist/index.js", "--transport", "stdio", "--username", "${DOCKER_HUB_USERNAME}"],
      "cwd": ".",
      "env": {
        "HUB_PAT_TOKEN": "${DOCKER_HUB_PAT}"
      },
      "disabled": true,
      "note": "Requires local installation. See: https://github.com/docker/hub-mcp"
    }
  }
}
